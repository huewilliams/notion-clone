{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "declaration": true,
    "typeRoots": ["dist/index.d.ts"],
    "target": "ES2015",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@src/*": ["src/*"]
    },
    "outDir": "dist"
  }
}
